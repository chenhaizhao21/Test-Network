static void send_respone_file_data(char *file) {
	char buf[512];
	int file_fd;
	if( (file_fd = open(file, O_RDONLY)) < 0 ) {
        printf("Open file error!!\n");
        return;
    }

    int r_size = 0;
    while( (r_size = read(file_fd, buf, 512)) > 0 ) {
        send_message(buf, r_size);
    }
	close(file_fd);
}

