static int check_request(req_info *header) {
	if( !header || !header->_req_line->path )
        return -1;
    char *file = header->_req_line->path;
    if( *file == '/' ) {
        file ++;
    }

    if( access(file, 4) < 0 ) { //检查读权限
        printf("The file is not exist\n");
        return -2;
    }

	return 0;
}

